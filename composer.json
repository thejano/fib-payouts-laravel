{
  "name": "thejano/fib-payouts-laravel",
  "description": "A Laravel package for FIB payouts using OAuth2 authentication.",
  "type": "library",
  "require": {
    "php": ">=8.1",
    "illuminate/support": ">=9.0"
  },
  "require-dev": {
    "pestphp/pest": "^2.0",
    "orchestra/testbench": "9.x-dev"
  },
  "autoload": {
    "psr-4": {
      "TheJano\\FibPayouts\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TheJano\\FibPayouts\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "TheJano\\FibPayouts\\Providers\\FibPayoutsServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "license": "MIT",
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
